  "scripts": {
    "build": "pnpm run lint && tsc --build tsconfig.prod.json",
    "clean-install": "rm -rf ./node_modules && rm -r pnpm-lock.yaml && pnpm install",
    "dev": "cross-env NODE_ENV=development node --env-file=./config/.env.development -r ts-node/register ./src",
    "dev:hot": "cross-env NODE_ENV=development nodemon --exec \"node --env-file=./config/.env.development -r ts-node/register ./src\" --watch ./src --ext .ts",
    "lint": "eslint",
    "start": "cross-env NODE_ENV=production node --env-file=./config/.env.production -r module-alias/register ./dist",
    "phantom-start": "cross-env NODE_ENV=production node -r module-alias/register ./dist",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20.6.0"
  },
